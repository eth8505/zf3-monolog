{
    "name": "eth8505/zf3-monolog",
    "description": "Zend Framework 3 monolog integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan-Simon Winkelmann",
            "email": "winkelmann@blue-metallic.de"
        }
    ],
    "require": {
        "php": "^7.1",
        "monolog/monolog": "^1.24 || ^2.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-stdlib": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Eth8505\\Monolog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eth8505Test\\Monolog\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "extra": {
        "zf": {
            "component": "Eth8505\\Monolog",
            "config-provider": "Eth8505\\Monolog\\ConfigProvider"
        }
    }
}
